{
  "size": 200,
  "name": "profiles",
  "schema": {
    "id": "datatype.uuid",
    "userId": "",
    "gender": "name.sex",
    "firstName": "name.firstName",
    "lastName": "name.lastName:prop.gender",
    "avatar": "unique:internet.avatar",
    "country": "use:Brasil",
    "countryCode": "use:BR",
    "age": "datatype.number:min_18+max_50",
    "state": "address.state",
    "timeZone": "use:America/Sao_Paulo",
    "website": "internet.url"
  }
}
